server {
    listen 80;
    server_name admin.nettineuvoja.dev;

    root /app/admin-ui/public;
    index index.html;

    location / {
        try_files $uri $uri/ /index.html?$query_string;
        sendfile off;
    }

    error_page 404 /404.html;

    error_page 500 502 503 504 /50x.html;
        location = /50x.html {
        root /usr/share/nginx/html;
    }
}
